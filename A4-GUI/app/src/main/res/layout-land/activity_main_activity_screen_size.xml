<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivityScreenSize">

    <TextView android:text="Normal Landscape Screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-29dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:onClick="getMyVersion"
        android:text="Get My Version" />

    <EditText
        android:id="@+id/numView1"
        android:layout_width="wrap_content"
        android:layout_height="97dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="489dp"
        android:layout_marginBottom="282dp"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/numView2"
        android:layout_width="210dp"
        android:layout_height="102dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="488dp"
        android:layout_marginBottom="179dp"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/answerView"
        android:layout_width="219dp"
        android:layout_height="109dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="237dp"
        android:layout_marginEnd="481dp"
        android:layout_marginBottom="49dp"
        android:ems="10"
        android:inputType="numberDecimal" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="226dp"
        android:layout_marginTop="237dp"
        android:layout_marginEnd="377dp"
        android:layout_marginBottom="56dp"
        app:srcCompat="@drawable/equals_ic_launcher" />

    <ImageButton
        android:id="@+id/addBtn"
        android:layout_width="wrap_content"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="384dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="238dp"
        android:layout_marginBottom="265dp"
        app:srcCompat="@drawable/plus_ic_launcher" />

    <ImageButton
        android:id="@+id/subBtn"
        android:layout_width="85dp"
        android:layout_height="81dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="567dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="264dp"
        app:srcCompat="@drawable/minus_ic_launcher" />

    <ImageButton
        android:id="@+id/multBtn"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="387dp"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="235dp"
        android:layout_marginBottom="138dp"
        app:srcCompat="@drawable/mult_ic_launcher" />

    <ImageButton
        android:id="@+id/divBtn"
        android:layout_width="89dp"
        android:layout_height="92dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="569dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="122dp"
        app:srcCompat="@drawable/divide_ic_launcher" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="391dp"
        android:layout_height="156dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="324dp"
        android:layout_marginTop="239dp"
        app:srcCompat="@drawable/mysvg" />


</RelativeLayout>
